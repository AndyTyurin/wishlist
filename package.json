{
  "name": "wishlist",
  "version": "0.0.1",
  "description": "Wishlist react application",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:AndyTyurin/wishlist.git"
  },
  "author": "Andy Tyurin <andy.tyurin@gmail.com>",
  "engines": {
    "node": ">=8.2.1"
  },
  "scripts": {
    "webpack-client": "node ./webpack/dev_server.js --platform=client",
    "webpack-server": "node ./webpack/dev_server.js --platform=server",
    "webpack-api": "node ./webpack/dev_server.js --platform=api",
    "build-development": "NODE_ENV=development npm run _build",
    "build-production": "NODE_ENV=production npm run _build",
    "lint": "npm run _lint-sass && npm run _lint-js",
    "release-patch": "npm version patch -m 'Bump patch version'",
    "release-minor": "npm version minor -m 'Bump minor version'",
    "release-major": "npm version major -m 'Bump major version'",
    "preversion": "npm run lint && npm _test",
    "postversion": "git push && git push --tags",
    "test-watch": "npm run _test -- --watch --growl",
    "coverage": "NODE_PATH=./src ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha \"src/**/__tests/*.spec.@(js|jsx)\" -- --ui tdd -R spec --compilers js:babel-register,css:ignore.js -r jsdom-global/register && open $(pwd)/coverage/lcov-report/index.html",
    "_build": "npm run _build-client && npm run _build-server && npm run _build-api",
    "_build-client": "platform=client visualize=1 node ./node_modules/webpack/bin/webpack",
    "_build-server": "platform=server node ./node_modules/webpack/bin/webpack",
    "_build-api": "platform=api node ./node_modules/webpack/bin/webpack",
    "_lint-sass": "node ./node_modules/.bin/sass-lint -v -q",
    "_lint-js": "eslint ./src/ --ext .js,.jsx",
    "_test": "./node_modules/mocha-webpack/bin/mocha-webpack --webpack-config webpack.mocha.js \"src/**/__tests/*.spec.@(js|jsx)\" --ui tdd",
    "_production": "NODE_ENV=production nodemon -L ./dist/server/server.js"
  },
  "pre-commit": [
    "lint"
  ],
